{
  "exp": {
    "id": "v1.0"
  },
  "data_dir": "~/data",
  "model":{
    "input_size": 270,
    "output_size":80,
    "input_chnl":3
  },
  "train":{
    "epochs": 80,
    "learning_rate": 0.0001
  },
  "callbacks":{
    "monitor": "val_loss",
    "save_best_only": false,
    "save_weights_only": true,
    "histogram_freq":1,
    "write_grads":true,
    "period":1,
    "tensorboard_write_graph": true
  }
}
